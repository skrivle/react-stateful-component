{
    "name": "react-stateful-component",
    "version": "0.0.1",
    "description": "Create stateful React components in a functional way.",
    "main": "lib/index.js",
    "files": [
        "lib"
    ],
    "scripts": {
        "lint": "eslint .",
        "flow": "flow check",
        "jest:watch": "jest --watch",
        "test": "npm run lint && npm run flow && jest",
