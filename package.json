{
    "name": "react-stateful-component",
    "version": "0.0.1",
    "description": "Create stateful React components in a functional way.",
    "main": "lib/index.js",
    "files": [
        "lib"
    ],
    "scripts": {
        "lint": "eslint .",
        "flow": "flow check",
        "test": "npm run lint && npm run flow && jest",
        "test:watch": "jest --watch",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "precommit": "lint-staged",
        "build:flow": "flow-copy-source src lib --ignore __test__/**/*",
        "build:babel": "babel src --out-dir lib --ignore __test__/",
        "build": "npm run build:flow && npm run build:babel"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vejersele/react-stateful-component.git"
    },
    "author": "Jelle Versele",
    "license": "MIT",
    "devDependencies": {
        "@storybook/addon-actions": "3.2.16",
        "@storybook/addon-links": "3.2.16",
        "@storybook/react": "3.2.16",
        "babel-cli": "6.26.0",
        "babel-eslint": "8.0.2",
        "babel-jest": "21.2.0",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-plugin-transform-object-rest-spread": "6.26.0",
        "babel-preset-env": "1.6.1",
        "babel-preset-react": "6.24.1",
        "enzyme": "3.2.0",
        "enzyme-adapter-react-16": "1.1.0",
        "eslint": "4.11.0",
        "eslint-config-prettier": "2.7.0",
        "eslint-plugin-flowtype": "2.39.1",
        "eslint-plugin-jest": "21.3.2",
        "eslint-plugin-react": "7.5.0",
        "flow-bin": "0.58.0",
        "flow-copy-source": "1.2.1",
        "husky": "0.14.3",
        "jest": "21.2.1",
        "jest-enzyme": "4.0.1",
        "lint-staged": "4.3.0",
        "prettier": "1.8.2",
        "raf": "3.4.0",
        "react": "16.1.1",
        "react-dom": "16.1.1",
        "react-test-renderer": "16.1.1",
        "uuid": "3.1.0"
    },
    "peerDependencies": {
        "react": "^15.0.0-0 || ^16.0.0-0"
    },
    "lint-staged": {
        "**/*.js": [
            "prettier --write",
            "git add"
        ]
    },
    "jest": {
        "setupTestFrameworkScriptFile": "./setup-tests.js"
    },
    "dependencies": {
        "invariant": "^2.2.2",
        "prop-types": "^15.6.0"
    }
}
